<?php
	
	$vetor[10] = array();
	echo '</BR>';
	echo"<b>=> EXERCÍCIO 25</b></BR></BR>";	
	echo"<b>=> VETOR ALEATÓRIO</b></BR>";	
	for($i = 0; $i < 10; $i++){
		$vetor[$i] = (float)(mt_rand(0, 100))/10;
	}
	echo '<ul></ul>';

	for($i = 0; $i < 10; $i++){
		echo number_format($vetor[$i],2)." | ";
